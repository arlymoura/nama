<%= form_for @import, url: dashboard_import_path, html: { multipart: true } do |f| %>
  <div class="field">
    <label class="label">Importar Arquivos</label>
    <div class="control">
      <%= f.file_field :import_file, class: 'input' %>
    </div>
  </div>

  <div class="field is-grouped">
    <div class="control">
      <%= link_to 'Voltar', :back, class: "button is-danger"%>
    </div>
    <div class="control">
      <%= f.submit 'Enviar', class: "button is-link" %>
    </div>
  </div>
<% end %>